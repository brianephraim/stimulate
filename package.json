{
  "name": "stimulate",
  "version": "0.0.8",
  "description": "animation utility",
  "main": "dist/stimulate.js",
  "files": [
		"dist",
		"src/library"
	],
  "scripts": {
    "build": "webpack --env=build && npm run commonjs2-import-example && npm run es2015-import-example",
    "dev": "webpack --progress --colors --watch --env=dev",
    "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "watch": "webpack-dev-server --progress --colors --env=watch  && echo 'http://localhost:8080/webpack-dev-server/'",
    "commonjs2-import-example": "browserify ./import-examples/commonjs2/index.js -o ./import-examples/commonjs2/index-compiled.js -d",
    "es2015-import-example": "webpack --config ./import-examples/es2015/webpack.config.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/defualt/stimulate.git"
  },
  "keywords": [
    "animation",
    "animate",
    "ui",
    "request",
    "animation",
    "frame",
    "easing",
    "tween",
    "timing"
  ],
  "author": "Brian Ephraim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/defualt/stimulate/issues"
  },
  "homepage": "https://github.com/defualt/stimulate",
  "tonicExampleFilename": "tonicExample.js",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eases": "^1.0.8",
    "ejs-compiled-loader": "^2.1.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.2.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "node-sass-json-importer": "^2.1.1",
    "react-prefixer": "^1.1.4",
    "requirejs": "^2.2.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "yargs": "^4.8.1"
  },
  "dependencies": {
    "raf": "^3.2.0"
  }
}
