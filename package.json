{
  "name": "stimulate",
  "version": "0.0.13",
  "description": "animation utility",
  "main": "dist/stimulate.js",
  "files": [
    "dist",
    "src/library",
    "tonicExample.js"
  ],
  "scripts": {
    "y": "devenv",
    "x": "node ./node_modules/devEnv/babel.start.js",
    "build": "./node_modules/.bin/babel-node ./node_modules/devEnv/webpack.start.js --env=build",
    "XXX_build": "./node_modules/devEnv/webpack --env=build && npm run commonjs2-import-example && npm run es2015-import-example",
    "XXX_commonjs2-import-example": "browserify ./import-examples/commonjs2/index.js -o ./import-examples/commonjs2/index-compiled.js -d",
    "XXX_es2015-import-example": "./node_modules/devEnv/webpack --config ./src/import-examples/es2015/webpack.config.babel.js",
    "XXX_watch": "./node_modules/devEnv/webpack-dev-server --progress --colors --env=watch  && echo 'http://localhost:8080/webpack-dev-server/'",
    "XXX_dev": "./node_modules/devEnv/webpack --progress --colors --watch --env=dev",
    "XXX_test": "babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha  --report lcovonly -- test/library.spec.js",
    "XXX_test-old": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "XXX_testwatch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "XXX_testbrowser": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js --withBrowser",
    "XXX_testsauce": "source .sauce_env && mocha --compilers js:babel-core/register --colors ./test/*.spec.js --localToSauce --useSauce",
    "XXX_saucelab": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js --useSauce",
    "XXX_coveralls": "cat ./coverage/lcov.info | coveralls",
    "XXX_coversend": "source .env && npm run test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "XXX_coverread": "babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha -- test/library.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/defualt/stimulate.git"
  },
  "keywords": [
    "animation",
    "animate",
    "ui",
    "request",
    "animation",
    "frame",
    "easing",
    "tween",
    "timing"
  ],
  "author": "Brian Ephraim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/defualt/stimulate/issues"
  },
  "homepage": "https://github.com/defualt/stimulate",
  "tonicExampleFilename": "tonicExample.js",
  "devDependencies": {
    "devEnv": "0.0.1"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "raf": "^3.3.2"
  }
}
